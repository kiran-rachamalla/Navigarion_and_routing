<mvc:View controllerName="kiran.Myapp.controller.App"
	xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" displayBlock="true">
	<App class="myAppDemoWT">
		<pages>
			<Page title="{i18n>homepageTitle}">
			<headerContent>
					<Button
						icon="sap-icon://hello-world"
						press="onOpenDialog"/>
				</headerContent>
				<content>
					<mvc:XMLView viewName="kiran.Myapp.view.HelloPanel"/>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>

